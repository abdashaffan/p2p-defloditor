{
  "name": "electron-webpack-quick-start-react",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "npm rebuild --runtime=electron \"--target=$(electron -v)\" \"--abi=$(electron --abi)\" --disturl=https://atom.io/download/atom-shell"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "hypermerge": "github:automerge/hypermerge",
    "hyperswarm": "^2.3.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.3",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "electron": "5.0.6",
    "electron-builder": "21.0.11",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "webpack": "~4.35.0"
  }
}
